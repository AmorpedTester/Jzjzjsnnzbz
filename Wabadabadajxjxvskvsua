local function skipCutscene()
    local player, cha, plr, hum = getPlayer()
    local bossSpawn = player.PlayerScripts.Shared.Modules.Effects:FindFirstChild("BossSpawn") 
    if bossSpawn then
        bossSpawn:Destroy()
      end
 end
 
 Tab:AddToggle({
	Name = "Skip Cutscene (Bug)",
	Default = false,
	Save = true,
    Flag = "Airjump",
	Callback = function(State)
	if State then
skipCutscene()
    end
  end    
})

