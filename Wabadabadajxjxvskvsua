local _G = {}
_G.Button3 = false -- Set the initial value to false

local function RunTween()
    local CFrameEnd = CFrame.new(-1593.79, 515.489, 606.865) -- Place your coords here
    local Time = speed2 -- Time in seconds (you can change this)
    local tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
    tween:Play()
    tween.Completed:Wait()
end

local Toggle = Tabs.Main:AddToggle("AutoKillGrottoBoss", {Title = "Auto Teleport to Boss", Default = false })

Toggle:OnChanged(function(Tswx)
    _G.Button3 = Tswx -- Set the global variable based on the toggle state
    if _G.Button3 then
        local startTime = tick()
        local timerx = tonumber(timerx) or 130 -- Default value or user input
        while _G.Button3 do
            if tick() - startTime >= timerx then
                RunTween()
                startTime = tick()
                print("Re-executed")
            end
            wait() -- This is crucial to avoid high CPU usage
        end
    end
end)

