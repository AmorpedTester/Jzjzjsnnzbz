        toggleCallback("Sell_Ultimate", Value)
    end)    

Tabs.Main:AddParagraph({
        Title = "Auto Bank",
        Content = "Warning: Auto Bank Can Transfer Equipped Weapon"
    })

function autoBank()
    while _G.Setting.AutoBank.Transfer_Enabled do
        local itemToBank = {
            [1] = "Inventory",
            [2] = "id", 
            [3] = 1
        }

        for _, item in pairs(InventoryController.Inventory.Items) do
            local id = item.Data.ItemID
            local rarity = item.DatabaseInfo.Rarity

            if _G.Setting.AutoBank.Transfer_Common and rarity == 'Common' then
                itemToBank[2] = id
            end

            if _G.Setting.AutoBank.Transfer_Rare and rarity == 'Rare' then
                itemToBank[2] = id
            end

            if _G.Setting.AutoBank.Transfer_Legendary and rarity == 'Legendary' then
                itemToBank[2] = id
            end

            if _G.Setting.AutoBank.Transfer_Mythical and rarity == 'Mythical' then
                itemToBank[2] = id
            end

            if _G.Setting.AutoBank.Transfer_Ultimate and rarity == 'Ultimate' then
                itemToBank[2] = id
            end
        end

        print('Bank Transfer Item:', itemToBank[2])

        
        game:GetService("ReplicatedStorage").Lobby.Remotes.BankTransfer:FireServer(unpack(itemToBank))

        wait(1)
    end
end

function toggleBankCallback(toggleName, value)
    _G.Setting.AutoBank[toggleName] = value

    if value then
        spawn(autoBank)
    end
end

local BankToggle = Tabs.Main:AddToggle("AutoBank", {Title = "Auto Transfer", Default = false })

BankToggle:OnChanged(function(Value)
    toggleBankCallback("Transfer_Enabled", Value)
end)

