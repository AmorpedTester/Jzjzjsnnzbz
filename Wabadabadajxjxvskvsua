        Title = "Destroy Map (old)",
        Description = "",
        Callback = function()
_G.loopsMap1 = true
while wait(1) do
if _G.loopsMap1 == true then
    for _, value in pairs(workspace:GetDescendants()) do
        if value:IsA("Part") and value.Parent.Name ~= game.Players.LocalPlayer.Name then
            value.Transparency = 1
        end
    end
end
end
end
    })


