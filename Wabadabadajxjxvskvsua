	Name = "White Screen Recommend to Reduce Lag"
})

Tab:AddButton({
	Name = "White-Screen (ON)",
	Callback = function()
game:GetService("RunService"):Set3dRenderingEnabled(false)
  	end    
})

Tab:AddButton({
	Name = "White-Screen (OFF)",
	Callback = function()
game:GetService("RunService"):Set3dRenderingEnabled(true)
  	end    
})

wait(2)

